    public static final Comparator<MysqlMonitorTO> HOSTNAME_ASC = new Comparator<MysqlMonitorTO>() {
        @Override
        public int compare(MysqlMonitorTO o1, MysqlMonitorTO o2) {
            if ((o1.getHostName()).compareTo(o2.getHostName()) < 0) {
                return 1;
            }
            if ((o1.getHostName()).compareTo(o2.getHostName()) > 0) {
                return -1;
            }
            return 0;
        }
    };

