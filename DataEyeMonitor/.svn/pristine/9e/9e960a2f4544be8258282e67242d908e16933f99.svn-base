package com.dataeye.omp.module.cmdb.summary;

import com.google.gson.annotations.Expose;

/**
 * @auther wendy
 * @since 2015/12/29 17:05
 */
public class Summary {

    @Expose
    private int id;

    //指标值
    @Expose
    private String name;

    //指标名称
    @Expose
    private int value;

    //指标占比
    @Expose
    private String percentage;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getValue() {
        return value;
    }

    public void setValue(int value) {
        this.value = value;
    }

    public String getPercentage() {
        return percentage;
    }

    public void setPercentage(String percentage) {
        this.percentage = percentage;
    }

    public Summary(int id, String name, int value){
        this.setId(id);
        this.setName(name);
        this.setValue(value);

    }

    public Summary(){}

}
